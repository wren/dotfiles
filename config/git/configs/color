# https://git-scm.com/docs/git-config#git-config-color
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = cyan
[color "diff"]
  meta = 227
  frag = cyan bold
  old = red
  new = green
  plain = dim normal
  commit = 227 bold
  whitespace = reverse red
[color "decorate"]
  branch = yellow
  remoteBranch = cyan
  tag = magenta
  stash = reverse magenta
  HEAD = yellow
[color "status"]
  header = magenta
  added = yellow reverse
  changed = yellow
  untracked = cyan
  header = white
  updated = green
  nobranch = bold blink red
  branch = yellow
  localBranch = yellow
  remoteBranch = cyan
  unmerged = red
[color "diff-highlight"]
  oldNormal = red
  oldHighlight = red 52
  newNormal = green
  newHighlight = green 22
