# https://git-scm.com/docs/git-config#git-config-color
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = cyan
[color "diff"]
  meta = 227
  frag = magenta bold
  old = red bold
  new = green bold
  plain = normal
  commit = 227 bold
  whitespace = reverse red
[color "decorate"]
  branch = yellow
  remoteBranch = cyan
  tag = magenta
  stash = reverse magenta
  HEAD = yellow
[color "status"]
  header =
  added = yellow reverse
  changed = green
  untracked = cyan
  header = white dim
  updated = yellow
  nobranch = red bold
  branch = cyan
  nobranch = red bold
  localBranch = yellow
  remoteBranch = cyan
  unmerged = red
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = red bold 52
  newNormal = green bold
  newHighlight = green bold 22
